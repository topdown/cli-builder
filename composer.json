{
  "name" :              "topdown/cli-builder",
  "description" :       "CLI Builder aims to help simplify building powerful CLI commands for PHP.",
  "authors" :           [
	{
	  "name" :  "Jeff Behnke",
	  "email" : "code@validwebs.com"
	}
  ],
  "minimum-stability" : "dev",
  "require" :           {
	"php" :              ">=7.1",
	"psr/http-message" : "^1.0"
  },
  "repositories" :      [
	{
	  "type" : "git",
	  "url" :  "https://github.com/topdown/cli-builder.git"
	}
  ],
  "require-dev" :       {
	"phpunit/phpunit" :           "^7",
	"squizlabs/php_codesniffer" : "^3"
  },
  "autoload" :          {
	"psr-4" : {
	  "cli_builder\\" : "src/"
	}
  }
}
